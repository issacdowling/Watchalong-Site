{
  "manifest_version": 2,
  "name": "Watchalong Manager",
  "version": "0.1",

  "description": "Allows others to watch Youtube videos with you",

  "icons": {
    "48": "icon.png"
  },
  "permissions": ["http://127.0.0.1:5000/submit"],

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*"],
      "js": ["watchalong-manager.js"]
    }
  ]
}
